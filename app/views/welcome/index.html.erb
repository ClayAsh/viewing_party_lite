<h3> Viewing Party </h3>
<% if current_user %>
  <%= button_to 'Log Out', logout_path, method: :delete %>
<% else %>
  <%= button_to 'Register', registration_path, method: :get %>
  <%= link_to 'Log In', login_path %>
<%end%>
<% if current_user %>
  <h5>Current Users:</h5>
  <% @users.each do |user| %>
    <%= user.email %></br>
  <%end%>
<%end%>
